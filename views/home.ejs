<%- include('partials/header'); %>
<h1>Home</h1>
<p><%= homeStartingContent %></p>
<% for(var i=posts.length - 1; i>=0; i--) %>
   <h2><%= post[i].postTitle %></h2>
   <p class="postBody"><%= _.truncate(post[i].postBody, {'length': 100, 'omission': ' ...'}) %></p>
   <a href="/posts/<%= post[i]._id %>">Read More</a>
<% } %>
<%- include('partials/footer'); %>
